import java.io.*;
import java.util.*;

class Main {
    public static void main(String[] args) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int N = Integer.parseInt(br.readLine());

        int[][] map = new int[N][N];

        for (int i = 0; i < N; i++) {
            map[i] = Arrays.stream(br.readLine().split(" "))
                           .mapToInt(Integer::parseInt)
                           .toArray();
        }

        int[] dx = {1, -1, 0, 0};
        int[] dy = {0, 0, 1, -1};
        int day = 0;

        while (true) {
            int[][] dec = new int[N][N];
            boolean hasTree = false;

            // 1ï¸âƒ£ ê°ì†ŒëŸ‰ ê³„ì‚°
            for (int i = 0; i < N; i++) {
                for (int j = 0; j < N; j++) {
                    if (map[i][j] > 0) {
                        hasTree = true;
                        for (int d = 0; d < 4; d++) {
                            int ni = i + dx[d];
                            int nj = j + dy[d];
                            if (ni < 0 || ni >= N || nj < 0 || nj >= N) continue;
                            if (map[ni][nj] == 0) dec[i][j]++;
                        }
                    }
                }
            }

            // ğŸŒŸ ì¢…ë£Œ ì¡°ê±´: ë” ì´ìƒ ë‚˜ë¬´ ì—†ìŒ
            if (!hasTree) break;

            // 2ï¸âƒ£ ê°ì†Œ ì ìš©
            for (int i = 0; i < N; i++) {
                for (int j = 0; j < N; j++) {
                    map[i][j] = Math.max(0, map[i][j] - dec[i][j]);
                }
            }

            day++;
        }

        System.out.println(day);
    }
}
